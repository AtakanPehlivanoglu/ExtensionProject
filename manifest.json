{
  "manifest_version": 2,
  "name": "My Cool Extension Test",
  "version": "1.0",
  "permissions": 
  [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs",
    "https://www.instagram.com/*"
  ],

"background": {

  "scripts": ["background.js"],
  "persistent": false

},

"content_scripts": [
  {
  "matches": ["https://www.instagram.com/*"],
  "js": [
    "lib/require/require-2.3.6.min.js",
    "lib/jquery/jquery-3.4.1.min.js" ,
    "content-operations/FollowBack.js"
  ]
},
{
  "matches": ["https://*.instagram.com/p*/"],
  "js": [
    "lib/require/require-2.3.6.min.js",
    "lib/jquery/jquery-3.4.1.min.js",
    "content-operations/LikeComments.js"
  ]
}
],


"browser_action": {

  "default_popup": "popup/popup.html",
  "default_icon": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }

}
}